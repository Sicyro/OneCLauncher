<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0" prefWidth="300.0" spacing="5.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.WarningWindowController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <Label fx:id="label" VBox.vgrow="ALWAYS" />
   <HBox alignment="BOTTOM_RIGHT" spacing="5.0" VBox.vgrow="ALWAYS">
      <Button fx:id="ok" defaultButton="true" mnemonicParsing="false" onAction="#onOk" text="Ок" />
      <Button fx:id="cansel" cancelButton="true" mnemonicParsing="false" onAction="#onCansel" text="Отмена" />
   </HBox>
</VBox>
